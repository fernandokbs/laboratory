<p id="notice"><%= notice %></p>

<h1>Type Services</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_services.each do |type_service| %>
      <tr>
        <td><%= type_service.name %></td>
        <td><%= link_to 'Show', type_service %></td>
        <td><%= link_to 'Edit', edit_type_service_path(type_service) %></td>
        <td><%= link_to 'Destroy', type_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Service', new_type_service_path %>
